public class egenOppgave {
	
	
	public static void main(String[] args) {
		
		int ant = 1;
		int tall2 = 1;
		int tall3 = 1;
		int tall4 = 1;
		
		kjor(ant, tall2, tall3, tall4);
	}
	
	static void kjor(int ant, int t2, int t3, int t4 ){
		ant ++;
		
		if (ant <= 100){
			System.out.println(t3);
			ant ++;
			regnNeste(t2, t3, t4);
		}
		
		else {
			System.out.println("Og det var det.");
		}
		

	}
	
	
	
	static void regnNeste(int t2, int t3, int t4){
		t4 = t3;
		t3 = t3 + t2;
	}
}
